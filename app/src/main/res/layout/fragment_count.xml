<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FFF"
    tools:context="com.sas.rh.reimbursehelper.fragment.CountFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#1882CE"
        android:gravity="center"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/common_constact"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/ll_top_bg"
            android:orientation="horizontal">

            <Button
                android:id="@+id/constact_group"
                style="@style/top_group"
                android:text="个人" />

            <Button
                android:id="@+id/constact_all"
                style="@style/top_all"
                android:text="企业" />
        </LinearLayout>

    </LinearLayout>

    <FrameLayout

        android:id="@+id/id_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></FrameLayout>

</LinearLayout>
